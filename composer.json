{
    "name": "klipper/doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine Extensions bundle of Klipper",
    "keywords": ["doctrine", "extensions", "validator", "unique entity"],
    "homepage": "https://github.com/klipperdev/doctrine-extensions-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@klipper.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0|^8.0.0",
        "symfony/framework-bundle": "^5.4.0",
        "klipper/doctrine-extensions": "^2.0.0"
    },
    "autoload": {
        "psr-4": { "Klipper\\Bundle\\DoctrineExtensionsBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0-dev"
        }
    }
}
